{
  "name": "i18n-polyglot",
  "description": "A React wrapper for node-polyglot",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": "goodhood-eu/i18n-polyglot",
  "bugs": "https://github.com/goodhood-eu/i18n-polyglot/issues",
  "version": "1.0.0-beta.0",
  "keywords": [
    "react",
    "polyglot",
    "i18n",
    "translation",
    "locale",
    "language",
    "localization"
  ],
  "files": [
    "lib/*.js"
  ],
  "main": "lib/index.js",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "lint": "eslint ./",
    "prepublishOnly": "npm test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "test": "TZ='GMT' mocha \"./{,!(node_modules)/**}/*.test.{js,jsx}\""
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-nebenan": "^1.0.0",
    "mocha": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "prop-types": "^15.7.2"
  },
  "dependencies": {
    "node-polyglot": "^2.4.0"
  }
}
